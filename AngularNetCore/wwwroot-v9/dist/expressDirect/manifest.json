{
  "manifest_version": 2,
  "name": "expressDirect extension",
  "description": "browser extension for expressDirect",
  "version": "1.0.0",
  "background": {
    "scripts": [ "background.js" ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "expressDirect extension"
  },
  "permissions": [ "tabs" ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
